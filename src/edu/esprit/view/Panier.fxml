<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="35.0" layoutY="25.0" text="Shopping Cart" />
      <TableView fx:id="cartTable" layoutX="25.0" layoutY="70.0" prefHeight="176.0" prefWidth="350.0">
         <columns>
            <TableColumn fx:id="nameColumn" prefWidth="200.0" text="Name" />
            <TableColumn fx:id="priceColumn" prefWidth="150.0" text="Price" />
         </columns>
      </TableView>
      <Button fx:id="checkoutButton" layoutX="299.0" layoutY="260.0" mnemonicParsing="false" text="Checkout" />
      <Button fx:id="continueShoppingButton" layoutX="161.0" layoutY="260.0" mnemonicParsing="false" text="Continue Shopping" />
      <TextField fx:id="totalPriceField" editable="false" layoutX="255.0" layoutY="32.0" prefWidth="120.0" />
      <Label layoutX="170.0" layoutY="35.0" text="Total Price:" />
   </children>
</AnchorPane>
