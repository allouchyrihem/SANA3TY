<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.controller.AfficherEventsController">
   <children>
      <Pane prefHeight="400.0" prefWidth="602.0">
         <children>
            <TableView fx:id="personsTable" layoutY="1.0" prefHeight="269.0" prefWidth="392.0">
              <columns>
                <TableColumn fx:id="NameColonne" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="AdresseColonne" prefWidth="75.0" text="Adresse" />
                  <TableColumn fx:id="DateColonne" prefWidth="75.0" text="Date" />
                  <TableColumn fx:id="DescriptionColonne" prefWidth="75.0" text="Description" />
                  <TableColumn fx:id="LinkColonne" prefWidth="75.0" text="Link" />
              </columns>
            </TableView>
            <Button fx:id="updatebuttonid" layoutX="77.0" layoutY="316.0" mnemonicParsing="false" onAction="#updatebtn" text="update" />
            <Button fx:id="deletebuttonid" layoutX="209.0" layoutY="316.0" mnemonicParsing="false" onAction="#deletebtn" text="delete" />
            <Button fx:id="addevid" layoutX="344.0" layoutY="316.0" mnemonicParsing="false" onAction="#addbtn" text="add" />
         </children>
      </Pane>
   </children>
</AnchorPane>
