<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.controller.SessionPanierController">
   <children>
      <HBox spacing="10">
         <ListView fx:id="produitsView" />
         <ListView fx:id="panierView" />
         <Button text="Ajouter au panier" onAction="#ajouterAuPanier" />
         <Button fx:id="acheterbtn" text="Acheter" onAction="#acheter" />
      </HBox>
   </children>
</AnchorPane>
